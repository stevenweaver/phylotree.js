{
  "name": "phylotree",
  "version": "1.0.0-alpha.21",
  "main": "build/phylotree.js",
  "bin": {
    "root-to-tip": "./src/cli/root-to-tip.js",
    "tip-date-extractor": "./src/cli/tip-date-extractor.js"
  },
  "keywords": [
    "phylogenetics",
    "phylogeny",
    "d3",
    "branching",
    "tree"
  ],
  "ignore": [
    "index.html"
  ],
  "dependencies": {
    "bootstrap": "4",
    "commander": "^4.1.1",
    "csv-stringify": "^5.3.6",
    "d3": "^5.7.0",
    "d3-hierarchy": "^1.1.8",
    "d3-line": "^0.0.1",
    "d3-time-format": "^2.1.3",
    "fast-csv": "4.0.2",
    "font-awesome": "4.x",
    "jquery": "3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "pretty-data": "^0.40.0",
    "puppeteer": "^2.1.1",
    "puppeteer-core": "^2.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "underscore": "1.x",
    "winston": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "http-server": "^0.12.1",
    "jsdoc": "^3.5.5",
    "jsdom": "16.1.0",
    "mocha": "^7.0.1",
    "prettier": "^1.14.2",
    "rollup": "^1.31.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-watch": "^4.3.1",
    "should": "^13.2.3",
    "source-map-support": "^0.5.9",
    "tape": "4",
    "uglify-js": "3.7.7",
    "yarn": "^1.9.4"
  },
  "scripts": {
    "docs": "sphinx-build -b html docs documentation",
    "lint": "eslint --ext .js --ignore-pattern data,docs,images,node_modules ./",
    "dev": "rm -rf build && mkdir build && rollup -m -c -w -f umd -o build/phylotree.js -- src/index.js",
    "build": "rm -rf build && mkdir build && rollup -m -c -f umd -o build/phylotree.js -- src/index.js",
    "watch": "tape-watch -r source-map-support/register test/*-test.js",
    "pretest": "rm -rf build && mkdir build && rollup -c -f umd -o build/phylotree.js -- src/index.js",
    "test": "tape -r source-map-support/register 'test/*-test.js'",
    "postpublish": "zip -j build/phylotree.js.zip -- LICENSE README.md build/phylotree.js build/phylotree.min.js build/phylotree.css"
  },
  "files": [
    "build/phylotree.css",
    "build/phylotree.js",
    "build/phylotree.min.js",
    "src/cli"
  ]
}
